<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.greenstock.repository.interfaces.SubscribeToAdvisorRepository"> 

	<select id="findByAdvisorIdAndUserId" resultType="SubscribeToAdvisor">
		select * from subscribe_to_advisor 
		where advisor_id = #{advisorId} 
		and user_id = #{userId}
	</select>
	


</mapper>